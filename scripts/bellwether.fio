[global]
direct=1
ioengine=libaio
randrepeat=0
group_reporting
time_based
ramp_time=30
runtime=14400


bs=512k
iodepth=8
#numjobs=8
size=90%
#rw=read
#rw=write
rw=randread
#rw=randwrite
rwmixread=100

[job1]
filename=/dev/sdc
[job2]
filename=/dev/sdd
[job3]
filename=/dev/sde
[job4]
filename=/dev/sdf
[job5]
filename=/dev/sdg
[job6]
filename=/dev/sdh
